{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Remote Debug",
            "preLaunchTask": "Debug Build",
            "executable": "${workspaceFolder}/build/blinky.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "serverpath": "${env:PICO_OPENOCD_PATH}/openocd",
            "cwd": "${env:PICO_OPENOCD_PATH}/scripts",
            "gdbPath": "${env:PICO_TOOLCHAIN_PATH}/bin/arm-none-eabi-gdb",
            "showDevDebugOutput": "parsed",
            "device": "RP2350",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/rp2350.cfg"
            ],
            "svdFile": "${workspaceFolder}/lib/pico-sdk/src/rp2350/hardware_regs/RP2350.svd",
            "runToEntryPoint": "main",
            "overrideLaunchCommands": [
                "monitor reset init",
                "load \"${workspaceFolder}/build/blinky.elf\""
            ],
            "openOCDLaunchCommands": [
                "adapter speed 5000"
            ]
        }
    ]
}
